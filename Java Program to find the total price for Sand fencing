import java.io.*;
class fence
{
public static void main(String args[])
{
int l,b,total,circumference;
l=164;
b=362;
circumference = 2*(l+b);
total = circumference*14;

System.out.println("The Circumference of the given = "+circumference);
System.out.println("The Total cost for Sand Fencing  = "+total);
}
}
